  { img: 'https://ieenormalsuperiormm.edu.co/wp-content/uploads/2024/02/SIMAT-300-140.png', href: 'https://sinai.net.co/Web/Default.aspx?ReturnUrl=%2f' },
  { img: 'https://ieenormalsuperiormm.edu.co/wp-content/uploads/2024/02/RECALH-300-140.png', href: 'https://sinai.net.co/Web/Default.aspx?ReturnUrl=%2f' },
  { img: 'https://ieenormalsuperiormm.edu.co/wp-content/uploads/2024/02/ICFES-300-140.png', href: 'https://sinai.net.co/Web/Default.aspx?ReturnUrl=%2f' },
  
  

